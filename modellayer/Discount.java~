package modellayer;
import java.util.*;

/**
 * Write a description of class Discount here.
 *
 * @author (Mathias)
 * @version (25-05-2018)
 */
public class Discount
{
    private Date timeFrom;
    private Date timeTo;
    private double percentage;

    public Discount(Date timeFrom, Date timeTo, double percentage)
    {
        this.timeFrom = timeFrom;
        this.timeTo = timeTo;
        this.percentage = percentage;
    }

    /**
     *Setters
     */
    public void setTimeTo(Date timeTo) { this.timeFrom = timeFrom;}  
    public void setTimeFrom(Date timeFrom) { this.timeFrom = timeFrom;}  
    public void setPercentage(double percentage) { this.percentage = percentage;}  

    /**
     *Getters
     */
    public Date getTimeTo() { return timeTo; }

    public Date getTimeFrom() { return timeFrom; }

    public double getPercentage() { return percentage; }
}